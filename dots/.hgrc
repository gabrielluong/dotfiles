[ui]
username = Gabriel Luong <gabriel.luong@gmail.com>
interface = curses
tweakdefaults = true

[defaults]
diff = -p -U 8
qdiff = -p -U 8
qnew = -U

[diff]
git = 1
unified = 8
showfunc = 1

[extensions]
mq = 
purge = 
reviewboard = /Users/oracle/.mozbuild/version-control-tools/hgext/reviewboard/client.py
bzexport = ~/.mozbuild/version-control-tools/hgext/bzexport
mqext = ~/.mozbuild/version-control-tools/hgext/mqext
push-to-try = ~/.mozbuild/version-control-tools/hgext/push-to-try
fsmonitor = 

[pager]
pager = less -FSRX
ignore = version, help, update, serve, record

[experimental]
clonebundles = true
